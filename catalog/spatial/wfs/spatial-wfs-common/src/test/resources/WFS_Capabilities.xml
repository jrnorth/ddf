      <wfs:OutputFormats>
        <wfs:Format>text/xml; subType=gml/3.1.1/profiles/gmlsf/1.0.0/0</wfs:Format>
      </wfs:OutputFormats>
      <ows:WGS84BoundingBox>
        <ows:LowerCorner>-114.80237639999996 31.369953340000052</ows:LowerCorner>
        <ows:UpperCorner>-109.04870079999995 36.99750922000004</ows:UpperCorner>
      </ows:WGS84BoundingBox>
